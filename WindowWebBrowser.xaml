<Window x:Class="odtwarzacz_audio_wpf.WindowWebBrowser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:odtwarzacz_audio_wpf"
        mc:Ignorable="d"
        Title="WindowWebBrowser" Height="450" Width="800">
    <Grid>
        <WebBrowser x:Name="WebBrowser" HorizontalAlignment="Left" Height="332" Margin="2,1,0,0" VerticalAlignment="Top" Width="788"/>
        <Button x:Name="ButtonGo" Content="▶" HorizontalAlignment="Left" Margin="683,349,0,0" VerticalAlignment="Top" Width="76" Height="54" FontSize="40" Click="ButtonGo_Click"/>
        <TextBox x:Name="WebBrowserURLAddress" HorizontalAlignment="Left" Height="49" Margin="12,350,0,0" TextWrapping="Wrap" Text="URL address" VerticalAlignment="Top" Width="666" FontFamily="Rockwell Nova Extra Bold" FontSize="30"/>

    </Grid>
</Window>
